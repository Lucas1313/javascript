<% with FeaturePanel %>
	<section class="feature-panel-slide" style="background:url({$FeatureImage.URL}) no-repeat;">

		<header class="headlineHolder">
			<h1 class="{$combineClass('Title1_Class')}">{$Title_1}</h1>
			<h1 class="{$combineClass('Title2_Class')}">{$Title_2}</h1>
			<h1 class="{$combineClass('Title3_Class')}">{$Title_3}</h1>
			<h1 class="{$combineClass('Title4_Class')}">{$Title_4}</h1>
		</header>

		<div class="bottom">
			<h2 class="{$Title_Class}">{$Subtitle_1}</h2>

			<div class="descriptionCopy">
				{$Description}
			</div><!--/.descriptionCopy-->

		    <% if $CTA_Title %>
		    <a class="btnSquare btnBlue" href="{$CTA_Link}">
		    	<div class="btnSpan arrow">
		    		<i>{$CTA_Title}</i>
		    	</div>
		    </a>
		    <% end_if %>

		</div><!--/.bottom-->

	</section><!--/.feature-panel-slide-->
<% end_with %>
<!-- mobile -->
<a class="corner-wrapper-image">
	<img class="corner" src="/Carousel/templates/img/laugh/animated-corner/kids/mobile-corner.png" />
</a>
<!-- end mobile -->
